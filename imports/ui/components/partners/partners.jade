template(name="partners")
	if Template.subscriptionsReady
		.panel.panel-default
			.panel-heading
				p.panel-title {{_ "ourPartners"}}
			.panel-body
				.row(style="display:flex; flex-wrap: wrap;")
					each partners
						.col-xs-6.col-sm-6.col-md-4.col-lg-3
							.thumbnail
								img(src="#{pictureUrl}" alt="#{name}")
								.caption
									h4 #{name}
	else
		+spinner