template(name="questions")
	.container
		.row
			h2 Qui suis-je ?
			form.col.s12
				.row
					.input-field.col.s6
						input#first_name.validate(type="text")
						label(for="first_name") First Name
					.input-field.col.s6
						input#last_name.validate(type="text")
						label(for="last_name") Last Name
				.row
					.input-field.col.s12
						input#password.validate(type="password")
						label(for="password") Password
				.row
					.input-field.col.s12
						input#email.validate(type="email")
						label(for="email") Email
				.row
					.col.s6.left-align
						a.previousQuestion.waves-effect.waves-light.btn Question précédente
					.col.s6.right-align
						a.nextQuestion.waves-effect.waves-light.btn Question suivante
					